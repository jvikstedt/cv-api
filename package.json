{
  "name": "cv-api",
  "private": true,
  "version": "1.0.0",
  "description": "CV-API",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --verbose --maxWorkers 1 --detectOpenHandles --forceExit",
    "repl": "NODE_OPTIONS=--experimental-repl-await ts-node src/repl.ts",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js --configName src/config/typeorm.config.ts seed",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.config.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.7.1",
    "@nestjs/bull": "^0.1.1",
    "@nestjs/cli": "^7.2.0",
    "@nestjs/common": "^7.1.2",
    "@nestjs/core": "^7.1.2",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.1.2",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/testing": "^7.1.2",
    "@nestjs/typeorm": "^7.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.13.0",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/nunjucks": "^3.1.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/puppeteer": "^3.0.1",
    "@types/ramda": "^0.27.6",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "bcrypt": "^4.0.1",
    "bull": "^3.14.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "config": "^3.3.1",
    "docx-templates": "^4.3.0",
    "elastic-builder": "^2.7.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "google-auth-library": "^6.0.4",
    "jest": "^26.0.1",
    "nunjucks": "^3.2.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "prettier": "^2.0.5",
    "puppeteer": "^4.0.1",
    "ramda": "^0.27.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
